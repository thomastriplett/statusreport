{
  "name": "statusreport",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.5.1",
    "ajv-keywords": "^3.2.0",
    "concurrently": "^3.5.1",
    "eslint-config-react-app": "^2.1.0",
    "express": "^4.16.3",
    "jwt-decode": "^2.2.0",
    "material-ui": "^0.20.0",
    "mysql": "^2.15.0",
    "node-sass-chokidar": "^1.3.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-autosuggest": "^9.3.4",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.3.2",
    "react-popup": "^0.9.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-table": "^6.8.6",
    "react-transition-group": "^2.3.1",
    "reactjs-popup": "^1.1.1",
    "serve": "^9.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/css -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/css -o src/css --watch --recursive",
    "devserver": "react-scripts start",
    "dev": "npm-run-all --parallel watch-css devserver",
    "server": "nodemon /src/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "serve --single ./build"
  },
  "homepage": "https://eipd.dcs.wisc.edu/statusreport",
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.0",
    "npm-run-all": "^4.1.3"
  }
}